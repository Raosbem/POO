{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Flask App (venv)",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "app",
        "FLASK_ENV": "development"
      },
      "args": [
        "run",
        "--no-debugger",
        "--no-reload"
      ],
      "jinja": true,
      "python": "${workspaceFolder}/venv/Scripts/python.exe"
    }
  ]
}
